{
    "name": "@rockket/root",
    "version": "2.0.0",
    "scripts": {
        "postinstall": "npx prisma generate --schema=./apps/rockket-backend/prisma/schema.prisma",
        "dev": "nx run-many -t serve -p rockket-web,rockket-backend",
        "dev:lan": "npm run local-ip && NG_APP_SERVER_BASE_URL=http://$(ipconfig getifaddr en0):3000 npm run dev -- --configuration local-network",
        "local-ip": "echo \"Local IP: $(ipconfig getifaddr en0), frontend listening on http://$(ipconfig getifaddr en0):4200, backend listening on http://$(ipconfig getifaddr en0):3000\"",
        "affected": "nx affected -t build,test,component-test,e2e",
        "lint": "nx run-many -t lint --verbose",
        "lint:fix": "npm run lint -- --fix",
        "format": "nx format",
        "fix": "npm run format && npm run lint:fix",
        "init-env": "nx run-many -t init-env"
    },
